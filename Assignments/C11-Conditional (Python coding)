height = float(input("What is your height in m?: "))
weight = float(input("What is your weight in kg?: "))
print("Your BMI is:", weight/height**2)
x= (weight/height**2)
if x<16: print("You're severely underweight")
elif x<=18.4: print("You're underweight")
elif x<=21.5: print("You're normal!")
elif x<=29.9: print("You're overweight")
elif x<=34.9: print("You're kinda overweight")
elif x<=39.9: print("You're severely obese")
elif x>40: print("You're morbidly obese")
